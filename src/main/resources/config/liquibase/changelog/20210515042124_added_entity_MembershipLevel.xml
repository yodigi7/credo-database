<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">

    <!--
        Added the entity MembershipLevel.
    -->
    <changeSet id="20210515042124-1" author="jhipster">
        <createTable tableName="membership_level">
            <column name="id" type="bigint">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="level" type="varchar(255)">
                <constraints nullable="false" unique="true" uniqueConstraintName="ux_membership_level__level" />
            </column>
            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here -->
        </createTable>
        <loadData
            file="config/liquibase/data/membership_level.csv"
            separator=";"
            tableName="membership_level">
            <column name="id" type="numeric"/>
            <column name="level" type="string"/>
            <!-- jhipster-needle-liquibase-add-loadcolumn - JHipster (and/or extensions) can add load columns here -->
        </loadData>
    </changeSet>

    <!-- jhipster-needle-liquibase-add-changeset - JHipster will add changesets here -->

